<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <title>
        @if (ViewBag.Title != null)
        {
@ViewBag.Title}
    </title>
    <meta content="width=device-width, initial-scale=1.0" name="viewport">
    <meta content="eCommerce HTML Template Free Download" name="keywords">
    <meta content="eCommerce HTML Template Free Download" name="description">

    <!-- Favicon -->
    <link href="img/favicon.ico" rel="icon">

    <!-- Google Fonts -->
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,400|Source+Code+Pro:700,900&display=swap" rel="stylesheet">

    <!-- CSS Libraries -->
    <link href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css" rel="stylesheet">
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.10.0/css/all.min.css" rel="stylesheet">
    <link href="~/User/lib/slick/slick.css" rel="stylesheet">
    <link href="~/User/lib/slick/slick-theme.css" rel="stylesheet">
    <link rel="apple-touch-icon" sizes="76x76" href="~/assets/img/Eida-designstyle-summer-m.png">
    <link rel="icon" type="image/png" sizes="96x96" href="~/assets/img/Eida-designstyle-summer-m.png">
    <!-- Template Stylesheet -->
    <link href="~/User/css/style.css" rel="stylesheet">
</head>

<body>
   
    @* Header *@
    @{await Html.RenderPartialAsync("~/Views/Shared/_UserHeader.cshtml");}
    @* End Header*@

    @RenderBody()

    @* Footer *@
    @{await Html.RenderPartialAsync("~/Views/Shared/_UserFooter.cshtml");}
    @* End Footer*@
    <div class="clearfix"></div>

    <!-- Back to Top -->
    <a href="#" class="back-to-top"><i class="fa fa-chevron-up"></i></a>

    <!-- JavaScript Libraries -->
    <script src="https://code.jquery.com/jquery-3.4.1.min.js"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.bundle.min.js"></script>
    <script src="~/User/lib/easing/easing.min.js"></script>
    <script src="~/User/lib/slick/slick.min.js"></script>

    <!-- Template Javascript -->
    <script src="~/User/js/main.js"></script>
</body>
</html>
